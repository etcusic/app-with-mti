<%= form_for :user, method: 'post' do |f| %>

    <%= f.label :email, "Email" %>
    <%= f.text_field :email %><br>

    <%= f.label :password, "Password" %>
    <%= f.password_field :password %><br>

    <%= f.submit "Sign In" %>

<% end %>

<br><br>

<div>
    <button><%= link_to "Home Page", '/' %></button>
</div>